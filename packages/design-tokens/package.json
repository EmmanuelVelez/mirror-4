{
  "name": "@knapsack-cloud/ap-sandbox-design-tokens",
  "description": "Associated Press Design Tokens",
  "version": "2.9.0",
  "main": "dist/design-tokens.json",
  "module": "dist/design-tokens.js",
  "style": "dist/design-tokens.scss",
  "scripts": {
    "build": "style-dictionary build --config ./style-dictionary.config.js",
    "start": "nodemon --watch ../../data/knapsack.design-tokens.json --exec 'yarn build'",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "style-dictionary": "2.10.3"
  },
  "repository": {
    "url": "https://github.com/knapsack-cloud/ap-sandbox",
    "directory": "packages/design-tokens"
  },
  "private": false,
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com"
  }
}
